extends layout

block content
  div(class='container')
    div(class='row')
      div(class='col s12 m4 l8')
        h1(class='shadow')= '座位安排表'
        div(id='list', class='collection z-depth-1')
  script(type="text/javascript").
    $.getJSON("/api/classes", function(data) {
      for (var d in data) {
        $("<a href='/class/"+d+"' class='collection-item'>"+data[d]+"</a>").appendTo('#list');
      }
    });
